Host *
  ServerAliveInterval 15

Host 192.168.*.*
  StrictHostKeyChecking no

Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User aur

Host 192.168.1.1
  PubkeyAcceptedAlgorithms +ssh-rsa
  HostkeyAlgorithms +ssh-rsa

Host ecco
  HostName 192.168.1.2
  User dangle

Host github.com
  Hostname ssh.github.com
  Port 443
