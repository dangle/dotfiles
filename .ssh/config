Host *
  ServerAliveInterval 15

Host 192.168.*.*
  StrictHostKeyChecking no

Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User aur

Host router
  HostName 192.168.1.1
  HostkeyAlgorithms +ssh-rsa
  User root

Host 192.168.255.1
  HostkeyAlgorithms +ssh-rsa
  User root

Host 192.168.254.1
  HostkeyAlgorithms +ssh-rsa

Host ecco
  HostName 192.168.1.2

Host github.com
  Hostname ssh.github.com
  Port 443
